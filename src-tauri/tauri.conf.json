{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "pollak-bufe",
  "version": "1.0.0",
  "identifier": "info.pollak.bufe",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "pollak-bufe",
        "label": "main",
        "width": 800,
        "height": 600,
        "resizable": false,
        "fullscreen": true,
        "decorations": false,
        "alwaysOnTop": true,
        "devtools": false,
        "closable": false,
        "focus": true,
        "skipTaskbar": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
